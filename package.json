{
  "name": "gradient-all",
  "version": "1.1.0",
  "description": "Gradient creation library running in the browser",
  "browser": "gradient.js",
  "module": "dist/gradient.mjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/afternoon2/gradient"
  },
  "scripts": {
    "dev": "rollup -c",
    "commit": "git-cz"
  },
  "keywords": [
    "css",
    "svg",
    "gradients",
    "colors",
    "chroma-js"
  ],
  "author": "Jakub Antolak <poprostuantolak@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^12.7.9",
    "bili": "^4.8.1",
    "commitizen": "^4.0.3",
    "ghooks": "^2.0.4",
    "jest": "^24.9.0",
    "lerna": "^3.16.4",
    "typescript": "^3.6.3"
  },
  "dependencies": {},
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "jest": {
      "coverageThreshold": {
        "global": {
          "branches": 50,
          "functions": 50,
          "lines": 50,
          "statements": 50
        }
      },
      "coverageDirectory": "./coverage",
      "collectCoverage": true
    }
  }
}
